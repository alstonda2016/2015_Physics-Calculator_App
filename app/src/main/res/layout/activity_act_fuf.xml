<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.david.physbun.actFUF"
    android:background="#212121">

    <TextView android:text="Ff  = µk * Fn" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="60sp"
        android:id="@+id/textView39"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#2196F3" />

    <EditText
        android:layout_width="220dp"
        android:layout_height="@dimen/n80"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText76"
        android:hint="Ff"
        android:background="#424242"
        android:layout_below="@+id/textView39"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="45sp" />

    <EditText
        android:layout_width="220dp"
        android:layout_height="@dimen/n80"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText77"
        android:hint=" µk"
        android:background="#424242"
        android:layout_below="@+id/editText76"
        android:layout_alignLeft="@+id/editText76"
        android:layout_alignStart="@+id/editText76"
        android:layout_marginTop="15dp"
        android:textSize="45sp" />

    <EditText
        android:layout_width="220dp"
        android:layout_height="@dimen/n80"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText78"
        android:hint="Fn"
        android:background="#424242"
        android:layout_below="@+id/editText77"
        android:layout_alignLeft="@+id/editText77"
        android:layout_alignStart="@+id/editText77"
        android:layout_marginTop="15dp"
        android:textSize="45sp" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/n80"
        android:text="Solve"
        android:id="@+id/button67"
        android:onClick="btnEnterFUF"
        android:layout_alignTop="@+id/button68"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#00E676"
        android:textSize="30sp" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/n80"
        android:text="Clear"
        android:id="@+id/button68"
        android:onClick="btnClearFUF"
        android:layout_below="@+id/editText78"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp"
        android:background="#2196F3"
        android:textSize="30sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ff ="
        android:id="@+id/textView49"
        android:textSize="45sp"
        android:layout_alignTop="@+id/editText76"
        android:layout_alignRight="@+id/textView51"
        android:layout_alignEnd="@+id/textView51"
        android:layout_marginTop="13dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" µk ="
        android:id="@+id/textView50"
        android:textSize="42sp"
        android:layout_alignTop="@+id/editText77"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fn ="
        android:id="@+id/textView51"
        android:textSize="45sp"
        android:layout_alignTop="@+id/editText78"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="13dp" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id">
    </com.google.android.gms.ads.AdView>

</RelativeLayout>
