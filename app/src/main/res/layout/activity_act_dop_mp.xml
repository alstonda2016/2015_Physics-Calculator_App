<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.david.physbun.actDopMP">

    <TextView android:text="Minus Plus" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/n70"
        android:id="@+id/textView32" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText44"
        android:layout_below="@+id/textView32"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="Fs" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText45"
        android:hint="Fo"
        android:layout_below="@+id/editText44"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText46"
        android:layout_below="@+id/editText45"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="V" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText47"
        android:layout_below="@+id/editText46"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="Vo" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText48"
        android:layout_below="@+id/editText47"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="Vs" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="@dimen/n80"
        android:text="New Button"
        android:id="@+id/button45"
        android:layout_below="@+id/editText48"
        android:layout_centerHorizontal="true"
        android:onClick="btnEnterdopmp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button55"
        android:layout_below="@+id/button45"
        android:layout_centerHorizontal="true"
        android:onClick="btnClearDopMP" />

</RelativeLayout>
