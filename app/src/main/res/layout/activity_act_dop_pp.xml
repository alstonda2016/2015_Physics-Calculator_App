<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.david.physbun.actDopPP">

    <TextView android:text="Plus Plus" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/n80"
        android:id="@+id/textView30" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText34"
        android:layout_below="@+id/textView30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="Fs" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText35"
        android:layout_below="@+id/editText34"
        android:layout_alignRight="@+id/editText34"
        android:layout_alignEnd="@+id/editText34"
        android:hint="Fo" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText36"
        android:layout_below="@+id/editText35"
        android:layout_alignRight="@+id/editText35"
        android:layout_alignEnd="@+id/editText35"
        android:hint="V" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText37"
        android:layout_below="@+id/editText36"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:hint="Vo" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText38"
        android:layout_below="@+id/editText37"
        android:layout_alignRight="@+id/editText37"
        android:layout_alignEnd="@+id/editText37"
        android:hint="Vs" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="@dimen/n80"
        android:text="Enter"
        android:id="@+id/button43"
        android:layout_below="@+id/editText38"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button57"
        android:layout_below="@+id/button43"
        android:layout_centerHorizontal="true"
        android:onClick="btnClearDopPP" />

</RelativeLayout>
