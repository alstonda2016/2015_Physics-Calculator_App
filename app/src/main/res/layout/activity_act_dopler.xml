<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.david.physbun.actDopler">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Doppler Effect"
        android:id="@+id/textView29"
        android:layout_marginTop="51dp"
        android:textSize="@dimen/n50"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/nnn120"
        android:text="F = Fo * (   (V + Vo)/(V + Vo)) "
        android:id="@+id/button39"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/button40"
        android:layout_alignEnd="@+id/button40"
        android:onClick="actDopCPP" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/nnn120"
        android:text="MP"
        android:id="@+id/button40"
        android:layout_below="@+id/button39"
        android:layout_alignLeft="@+id/textView29"
        android:layout_alignStart="@+id/textView29"
        android:onClick="actDopcMP" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/nnn120"
        android:text="PM"
        android:id="@+id/button41"
        android:layout_alignTop="@+id/button39"
        android:layout_alignLeft="@+id/button42"
        android:layout_alignStart="@+id/button42"
        android:onClick="actDopcPM" />

    <Button
        android:layout_width="@dimen/nnn120"
        android:layout_height="@dimen/nnn120"
        android:text="MM"
        android:id="@+id/button42"
        android:layout_below="@+id/button41"
        android:layout_alignRight="@+id/textView29"
        android:layout_alignEnd="@+id/textView29"
        android:onClick="actDopCMM" />
</RelativeLayout>
